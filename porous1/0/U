FoamFile
{
  version     2.0;
  format      ascii;
  class       volVectorField;
  object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
  inletWalls
  {
    type       directionMixed;
    refValue   uniform (0 0 0);
    refGradient   uniform (0 0 0);
    valueFraction  uniform (1 0 0 0 0 1);
    value     uniform (0 0 0);
  }
  outletWalls
  {
    type            zeroGradient;
  }
  fixedWalls
  {
    type            slip;
  }
}


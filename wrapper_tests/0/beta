FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      beta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1.0;

boundaryField
{
  topWall
  {
    type            zeroGradient;
  }

  fixedWalls
  {
    type            fixedValue;
    value           uniform 0;
  }

  frontAndBack
  {
    type            fixedValue;
    value           uniform 0;
  }
}

